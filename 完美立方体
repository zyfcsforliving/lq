#include<stdio.h>
#include<iostream>
using namespace std;
int main()
{
    int N;
    scanf("%d",&N);
    for(int a=2;a<=N;a++)
    for(int b=2;b<a;b++)
    for(int c=b;c<a;c++)
    for(int d=c;d<a;d++)
    if(a*a*a==b*b*b+c*c*c+d*d*d)
    printf("Cube=%d,truple=(%d,%d,%d)\n",a,b,c,d);
    return 0;
}

